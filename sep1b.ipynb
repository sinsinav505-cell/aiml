   "source": [
    "l=[\"Python is fun in India\"]\n",
    "print(l)\n",
    "m=[]\n",
    "l1=[]\n",
    "for i in l:\n",
    "    m=i.split()\n",
    "    for j in m:\n",
    "        for k in j:\n",
    "            k1=k[0].upper()\n",
    "            if k1==k[0]:\n",
    "                l1.append(j)\n",
    "print(l1)\n",
    "                "
